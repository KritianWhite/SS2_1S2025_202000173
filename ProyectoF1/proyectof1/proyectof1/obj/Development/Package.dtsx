#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{

    private Dictionary<string, int> codigoClienteContador = new Dictionary<string, int>();
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        if (!Row.CodigoCliente_IsNull)
        {
            string codigo = Row.CodigoCliente;

            if (codigoClienteContador.ContainsKey(codigo))
            {
                int conteo = codigoClienteContador[codigo];
                Row.CodigoCliente = $"{codigo}_D{conteo}".ToString();
                codigoClienteContador[codigo]++;
            }
            else
            {
                codigoClienteContador[codigo] = 1;
                // Se mantiene el valor original en la primera aparici�n
            }
        }
    }

}
