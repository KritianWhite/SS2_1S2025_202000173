using System;
using System.Data;
using System.Globalization;
using System.Text.RegularExpressions;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        DateTime tempFecha;
        if (!DateTime.TryParseExact(Row.Fecha, "dd/MM/yyyy", CultureInfo.InvariantCulture, DateTimeStyles.None, out tempFecha))
        {
            Row.Fecha = new DateTime(0001, 01, 01).ToString("yyyy/MM/dd");
        }
        else
        {
            Row.Fecha = tempFecha.ToString("yyyy/MM/dd");
        }

        Row.NumeroCliente = LimpiarNumeros(Row.NumeroCliente).ToString();

        Row.Unidades = LimpiarNumeros(Row.Unidades).ToString();

        Row.Vacacionista = LimpiarNumeros(Row.Vacacionista).ToString();

        Row.PrecioUnitario = LimpiarDecimales(Row.PrecioUnitario).ToString(CultureInfo.InvariantCulture);

    }

    private ulong LimpiarNumeros(string valor)
    {
        if (string.IsNullOrEmpty(valor))
        {
            return 0;
        }

        string limpio = Regex.Replace(valor, "[^0-9]", "");

        return string.IsNullOrEmpty(limpio) ? 0 : Convert.ToUInt64(limpio);

    }

    private decimal LimpiarDecimales(string valor)
    {
        if (string.IsNullOrWhiteSpace(valor))
        {
            return 0.0m;
        }

        string limpio = Regex.Replace(valor, "[^0-9.]", "");

        return decimal.TryParse(limpio, NumberStyles.AllowDecimalPoint, CultureInfo.InvariantCulture, out decimal resultado) ? resultado : 0.0m;

    }

}
